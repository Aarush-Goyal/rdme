{
  "author": "ReadMe <support@readme.io> (http://readme.io)",
  "name": "rdme",
  "description": "ReadMe's API CLI",
  "bin": {
    "rdme": "rdme.js"
  },
  "tags": [
    "api",
    "apis",
    "swagger",
    "open api initiative",
    "open api spec",
    "oai",
    "oas",
    "apidoc",
    "microservice",
    "documentation"
  ],
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/rdme.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/rdme/issues"
  },
  "main": "index.js",
  "dependencies": {
    "colors": "^1.1.2",
    "config": "^1.30.0",
    "minimist": "^1.2.0",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.3",
    "nock": "^9.2.3",
    "prettier": "^1.12.1"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "prettier": "prettier --list-different \"./**/**.js\"",
    "pretest": "npm run lint && npm run prettier",
    "test": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./test/set-node-env"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 80,
        "lines": 80,
        "statements": 75
      }
    }
  }
}
